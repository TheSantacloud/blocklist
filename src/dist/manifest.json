{
  "name": "blocklist",
  "version": "1.1.0",
  "description": "Privately manage a block list for websites",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "webNavigation",
    "scripting",
    "background"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/icon_color_128.png",
    "48": "assets/icon_color_128.png",
    "128": "assets/icon_color_128.png"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "commands": {
    "enable_blocklist": {
      "suggested_key": {
        "default": "Ctrl+Shift+B",
        "mac": "MacCtrl+Shift+B"
      },
      "description": "Block sites"
    }
  },
  "content_scripts": [
    {
      "js": [
        "assets/content.js-loader-De8s3uWf.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/content.js-BucE-y7v.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
